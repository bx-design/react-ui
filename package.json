{
  "name": "@bx-design/react-ui",
  "version": "1.0.0-canary.9",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.min.js",
  "repository": "git@github.com:bx-design/react-ui.git",
  "author": "BX Creative Studio",
  "license": "MIT",
  "prettier": "@bx-design/prettier-config",
  "keywords": [
    "blue express",
    "blue express ui",
    "bx",
    "bx design",
    "bx ui",
    "bx components",
    "components",
    "react components",
    "react ui"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clear": "rm -rf ./dist ./esm",
    "build:esm": "babel --config-file ./scripts/babel.config.js --extensions \".js,.ts,.tsx\" ./components --out-dir ./esm --ignore \"**/__tests__/*.spec.tsx,**/__tests__/__snapshots__/*,**/*.d.ts\"",
    "build:webpack": "webpack --config scripts/webpack.config.js",
    "build:types": "ttsc -p ./scripts",
    "build": "yarn clear && yarn build:esm && yarn build:webpack && yarn build:types",
    "lint": "eslint components",
    "lint:fix": "eslint --fix components",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "files": [
    "/dist",
    "/esm"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-react-jsx": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@bx-design/prettier-config": "1.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/styled-jsx": "^2.2.8",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.2.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^27.0.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.4",
    "typescript-transform-paths": "2.1.0",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "styled-jsx": "3.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
